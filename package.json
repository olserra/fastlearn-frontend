{
  "name": "startd-theme",
  "description": "Boostio landing page",
  "version": "1.0.0",
  "private": true,
  "author": "Otavio Serra",
  "type": "commonjs",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript",
    "tailwind",
    "theme",
    "starter",
    "gpt4",
    "pinecone",
    "typescript",
    "nextjs",
    "langchain",
    "law",
    "legal",
    "pdf",
    "openai"
  ],
  "scripts": {
    "dev": "next",
    "build": "npx prisma generate next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "npx prisma generate && next build",
    "ingest": "tsx -r dotenv/config src/scripts/ingest-data.ts"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn add lint --fix",
      "yarn add format"
    ]
  },
  "dependencies": {
    "@fontsource/inter": "4.2.1",
    "@heroicons/react": "^1.0.6",
    "@magic-sdk/admin": "^2.0.0",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@pinecone-database/pinecone": "^0.1.6",
    "@prisma/client": "^5.0.0",
    "@radix-ui/react-accordion": "^1.1.1",
    "@react-pdf/renderer": "^3.1.12",
    "@svgr/webpack": "5.5.0",
    "@typeform/embed-react": "1.2.4",
    "@types/formidable": "^3.4.1",
    "@types/react-swipeable": "^5.2.0",
    "@vercel/postgres": "^0.4.0",
    "ai": "^2.1.25",
    "axios": "^1.4.0",
    "chart.js": "^4.3.3",
    "clsx": "^1.2.1",
    "dotenv": "^16.3.1",
    "formidable": "^3.5.0",
    "js-cookie": "^3.0.5",
    "langchain": "0.0.82",
    "lucide-react": "^0.125.0",
    "magic-sdk": "^18.4.1",
    "next": "^13.4.10",
    "next-auth": "^4.22.3",
    "next-compose-plugins": "^2.2.1",
    "next-env": "^1.1.1",
    "next-seo": "^4.29.0",
    "node-fetch": "^3.3.1",
    "openai": "^3.3.0",
    "openai-edge": "^1.2.2",
    "opencommit": "^2.4.2",
    "pdf-parse": "1.1.1",
    "rc-table": "^7.32.1",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-markdown": "^8.0.5",
    "react-syntax-highlighter": "^15.5.0",
    "react-use": "^17.4.0",
    "recharts": "^2.7.2",
    "sharp": "^0.32.4",
    "tailwind-merge": "^1.10.0",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.3",
    "twind": "0.15.4",
    "vis": "^4.21.0-EOL",
    "vis-timeline": "^7.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/gtag.js": "^0.0.12",
    "@types/node": "^18.14.6",
    "@types/react": "^18.2.16",
    "@types/react-dom": "17.0.1",
    "@types/react-helmet": "6.1.5",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@typescript-eslint/parser": "^5.54.0",
    "autoprefixer": "^10.4.14",
    "commitizen": "4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "postcss": "^8.4.26",
    "prettier": "2.2.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^4.9.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
